      <script>
  const urlMapping = {"^/bookmarks$":"/my-bookmarks","^/courses$":"/courses","^/courses/all$":"/courses","^/courses/([^/]+)$":"/courses/$1","^/courses/((?:tech|topic):[^/]+/(?:tech|topic):[^/]+)$":"/courses/$1","^/groups(\\?.+)$":"/groups$1","^/groups$":"/groups","^/notifications$":"/notifications","^/practice$":"/practice","^/projects$":"/projects","^/projects/([^/]+)$":"/projects/$1","^/search$":"/search","^/signal$":"/assessments","^/tracks/career$":"/career-tracks","^/tracks/skill$":"/skill-tracks"};
  const mainAppPatterns = Object.keys(urlMapping);

  function getLearnHubUrl(href) {
    const pathname = href.replace(location.origin, '');
    if (pathname.indexOf('://') > -1) return false; // other domain

    let matches = false;
    let pattern = '';
    for (let i = 0; i < mainAppPatterns.length; i++) {
      const currentPattern = mainAppPatterns[i];
      const result = pathname.match(currentPattern);
      if (result) {
        matches = result;
        pattern = currentPattern;
        break;
      }
    }

    if (matches) {
      const replace = urlMapping[pattern];
      const learnHubUrl = matches[1]
        ? replace.replace('$1', matches[1])
        : replace;
      return learnHubUrl;
    }
    return false;
  }

  $('body').on('click', 'a', function (e) {
    // prevents renavigating to the same url
    if (window.location.href === this.href) return;

    const learnHubUrl = getLearnHubUrl(this.href);
    if (learnHubUrl) {
      e.preventDefault();

      const url = `${this.href}?embedded=true`;

      window.parent.postMessage(
        {
          payload: {
            learnHubUrl,
            type: 'NAVIGATION',
            url,
          },
          source: 'dc-main-app',
          sourceName: window.name,
        },
        '*',
      );
    }
  });

  // always send the current document.title
  const learnHubUrl = getLearnHubUrl(window.location.href);
  window.parent.postMessage(
    {
      payload: {
        learnHubUrl,
        title: document.title,
        type: 'SYNC_TITLE_AND_URL',
      },
      source: 'dc-main-app',
      sourceName: window.name,
    },
    '*',
  );
</script>

