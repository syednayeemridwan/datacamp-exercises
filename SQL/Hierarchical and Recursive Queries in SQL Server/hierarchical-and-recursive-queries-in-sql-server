    function inIframe() {
      // we are in the iframe, also prevents other from putting our website in an iframe
      try {
        return window.self !== window.top;
      } catch (e) {
        // deem it as an iframe (origin policy indicates it's loaded from an iframe)
        return true;
      } 
    }

    function sendDataLayerPush() {
      var dataLayerContent = [{"event":"view_course_page","content_id":16781}];
      if (typeof window['dataLayer'] === 'undefined') {
        window['dataLayer'] = [dataLayerContent];
      } else {
        for(var i=0; i < dataLayerContent.length; i++) {
          window['dataLayer'].push(dataLayerContent[i]);
        } 
      } 
    }

    if(inIframe()) {
      try {
        window.parent.postMessage({ 
          source: 'dc-main-app', 
          sourceName: window.name, 
          payload: {
            type: 'DATA_LAYER_PUSH', 
            body: [{"event":"view_course_page","content_id":16781}]
          }
        }, '*');
      } catch (e) {
        // if window still fails, send datalayer
        sendDataLayerPush();
      } 
    } else {
      sendDataLayerPush();
    }
</script>
